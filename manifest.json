{
	"manifest_version": 2,
	"name": "ELasticHQ Extension",
	"description": "www.elastichq.org",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	]
}

